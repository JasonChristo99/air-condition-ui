<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.airconditionui">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".views.options.OptionSwingActivity"
            android:label="@string/option_swing" />
        <activity
            android:name=".views.options.OptionTurboMode"
            android:label="@string/option_turbo" />
        <activity
            android:name=".views.options.OptionModeActivity"
            android:label="@string/option_mode" />
        <activity
            android:name=".views.options.OptionTimerActivity"
            android:label="@string/option_timer" />
        <activity
            android:name=".views.OptionsActivity"
            android:label="@string/options" />
        <activity
            android:name=".views.AppPreferencesActivity"
            android:label="@string/preferences" />
        <activity
            android:name=".views.options.OptionEcoModeActivity"
            android:label="@string/option_eco" />
        <activity
            android:name=".views.options.OptionSleepModeActivity"
            android:label="@string/option_sleep" />
        <activity
            android:name=".views.options.OptionFanSpeedActivity"
            android:label="@string/option_fan" />
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".views.MainActivity" />
    </application>

</manifest>